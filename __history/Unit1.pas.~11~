procedure TForm1.Button1Click(Sender: TObject);

begin
    AssignFile(f, 'xx.txt');
    Reset(f);
    while (not EOF(f)) do begin
      Readln(f, s);
    CloseFile(f);
    writeln(s);
end;

end;

procedure TForm1.Button2Click(Sender: TObject);
var intkont: double;
    a, b, c: integer;
    d: double;
begin
    intkont:= StrToFloat(e0.Text);

    e2.Text := FloatToStr(intkont);

    a:=1;
    b:=1;
